<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="root" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginPageController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="400.0" prefWidth="600.0">
         <children>
            <VBox spacing="20.0">
               <children>
                  <Label alignment="TOP_CENTER" contentDisplay="CENTER" prefHeight="22.0" prefWidth="552.0" text="Welcome To GigiKala" textAlignment="CENTER" textFill="#3a0cad" />
                  <TextField fx:id="username_field" promptText="username" />
                  <PasswordField fx:id="password_field" promptText="password" />
                  <Button fx:id="login_button" mnemonicParsing="false" prefHeight="25.0" prefWidth="627.0" text="Login"  />
                  <Button fx:id="goto_signup_button" mnemonicParsing="false" prefHeight="25.0" prefWidth="627.0" text="Signup"  />
               </children>
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="100.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
